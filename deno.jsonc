{
  "tasks": {
    "compile": "deno compile --allow-net --allow-read --allow-write --allow-env --allow-run --allow-ffi --unstable --output $INIT_CWD/deliverymetrics$SUFFIX $EXTRA_ARGS ./mod.ts",
    "dev:cli": "nice -20 deno task test --fail-fast --watch --no-clear-screen ./cli",
    "dev:fixtures": "deno run -A dev-fixtures/mod.ts",
    "dev:libs": "nice -20 deno task test --fail-fast --watch --no-clear-screen ./libs",
    "dev:utils": "nice -20 deno task test --fail-fast --watch --no-clear-screen ./utils",
    "lint": "deno run -A bin/fmt-imports.ts . --check && deno fmt --check && deno check mod.ts dev-fixtures/mod.ts bin/fmt-imports.ts && deno lint",
    "lint:fix": "deno run -A bin/fmt-imports.ts . && deno fmt && deno check mod.ts dev-fixtures/mod.ts bin/fmt-imports.ts && deno lint",
    "run": "deno run --unstable -A mod.ts",
    "test": "deno test --allow-read --allow-write --allow-env --allow-run --allow-ffi --unstable --check --parallel",
    "typecheck": "deno check --unstable mod.ts dev-fixtures/mod.ts bin/fmt-imports.ts"
  },
  "lint": {
    "include": ["bin","cli", "dev-fixtures", "libs", "utils"]
  },
  "fmt": {
    "include": ["bin","cli", "dev-fixtures", "libs", "utils"],
    "lineWidth": 120,
    "semiColons": false
  },
  "imports": {
    "aloedb": "https://deno.land/x/aloedb@0.9.0/mod.ts",
    "csv": "https://deno.land/x/csv@v0.7.5/mod.ts",
    "dev:asserts": "https://deno.land/std@0.199.0/testing/asserts.ts",
    "dev:conditional-type-checks": "https://deno.land/x/conditional_type_checks@1.0.6/mod.ts",
    "dev:mock": "https://deno.land/std@0.199.0/testing/mock.ts",
    "dev:mock-fetch": "https://deno.land/x/mock_fetch@0.3.0/mod.ts",
    "dev:time": "https://deno.land/std@0.199.0/testing/time.ts",
    "equal": "https://deno.land/x/equal@v1.5.0/mod.ts",
    "event": "https://deno.land/x/event@2.0.1/mod.ts",
    "kia": "https://deno.land/x/kia@0.4.1/kia.ts",
    "parse-link-header": "https://cdn.jsdelivr.net/gh/bryik/deno-parse-link-header@v0.1.1/parseLinkHeader.ts",
    "progress": "https://deno.land/x/progress@v1.3.4/mod.ts",
    "run-with-limit": "https://denopkg.com/alextes/run-with-limit/mod.ts",
    "semaphore": "https://deno.land/x/semaphore@v1.1.2/mod.ts",
    "slugify": "https://deno.land/x/slugify@0.3.0/mod.ts",
    "std:color": "https://deno.land/std@0.199.0/fmt/colors.ts",
    "std:datetime": "https://deno.land/std@0.199.0/datetime/mod.ts",
    "std:debounce": "https://deno.land/std@0.199.0/async/debounce.ts",
    "std:deep-merge": "https://deno.land/std@0.199.0/collections/deep_merge.ts",
    "std:distinct": "https://deno.land/std@0.199.0/collections/distinct.ts",
    "std:fs": "https://deno.land/std@0.199.0/fs/mod.ts",
    "std:group-by": "https://deno.land/std@0.199.0/collections/group_by.ts",
    "std:http-status": "https://deno.land/std@0.199.0/http/http_status.ts",
    "std:io": "https://deno.land/std@0.199.0/io/mod.ts",
    "std:log": "https://deno.land/std@0.199.0/log/mod.ts",
    "std:path": "https://deno.land/std@0.199.0/path/mod.ts",
    "std:streams": "https://deno.land/std@0.199.0/streams/mod.ts",
    "std:yaml": "https://deno.land/std@0.199.0/yaml/mod.ts",
    "tea-semver": "https://deno.land/x/libtea@v0.10.3/src/utils/semver.ts",
    "yargs": "./yargs.ts",
    "zod": "https://deno.land/x/zod@v3.17.10/mod.ts"
  }
}
